datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Request {
  id              Int       @id @default(autoincrement())
  date            DateTime
  text            String
  token           String    @unique
  country         String
  image           String?
  transport_choice String
  name            String
  hashtags        String?
  level           Int
  is_form_generated Boolean
}
